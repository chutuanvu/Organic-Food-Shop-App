<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.android_btl.Fragment.UserHomeFragment"
        tools:layout="@layout/user_fragment_home" />

    <fragment
        android:id="@+id/nav_product"
        android:name="com.example.android_btl.Fragment.UserItemFragment"
        tools:layout="@layout/user_fragment_product" />

    <fragment
        android:id="@+id/nav_favorite"
        android:name="com.example.android_btl.Fragment.UserFavoriteFragment"
        tools:layout="@layout/user_fragment_favorite" />
    <fragment
        android:id="@+id/nav_cart"
        android:name="com.example.android_btl.Fragment.UserCartFragment"
        tools:layout="@layout/user_fragment_cart" />
    <fragment
        android:id="@+id/nav_history"
        android:name="com.example.android_btl.Fragment.UserHistoryFragment"
        tools:layout="@layout/user_fragment_history" />
    <fragment
        android:id="@+id/nav_infomation"
        android:name="com.example.android_btl.Fragment.UserInformationFragment"
        tools:layout="@layout/user_fragment_information" />
    <fragment
        android:id="@+id/nav_help"
        android:name="com.example.android_btl.Fragment.UserHelpFragment"
        tools:layout="@layout/user_fragment_help" />
</navigation>